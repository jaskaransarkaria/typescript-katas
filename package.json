{
  "name": "typescript-kata",
  "version": "1.0.0",
  "description": "A template repo where you can solve katas",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint . --ext .ts --fix",
    "test": "npx nyc ava",
    "test:watch": "npx ava --watch",
    "build": "npx tsc",
    "start": "node build/src/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/node": "^15.12.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "ava": "^3.15.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "prettier": "^2.3.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "ava": {
    "files": [
      "src/**/*.spec.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "verbose": true
  }
}
